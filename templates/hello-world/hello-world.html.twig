{% extends 'base.html.twig' %}

{% block title %}Hello world{% endblock %}

{% block body %}

    {% if mee %}
    <ul>
        <h1>Hello World</h1>
        {% for mee in mee %}
            <p>my name is {{ mee.name }}</p>
            <a href="details/{{ mee.id }}">click to know more about me</a>
        {% endfor %}
    </ul>
    {% else %}
      <h1>No users in database</h1>
    {% endif %}

    {{ form_start(form) }}
    {{ form_widget(form) }}
    {{ form_end(form) }}
{% endblock %}
